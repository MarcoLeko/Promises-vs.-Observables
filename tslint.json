{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "quotemark": [
            true,
            "single"
        ],
        "no-console": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "only-arrow-functions": false,
        "trailing-comma": false
    },
    "rulesDirectory": []
}